{
  "name": "hawk-e2e",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "app/department/hawk/hawk-e2e/src",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "app/department/hawk/hawk-e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "hawk:serve:development"
      },
      "configurations": {
        "production": {
          "devServerTarget": "hawk:serve:production"
        },
        "ci": {
          "devServerTarget": "hawk:serve-static"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["app/department/hawk/hawk-e2e/**/*.{js,ts}"]
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["hawk"]
}
