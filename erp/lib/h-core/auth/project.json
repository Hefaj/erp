{
  "name": "auth",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "lib/h-core/auth/src",
  "prefix": "erp",
  "tags": ["auth"],
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "lib/h-core/auth/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "lib/h-core/auth/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "lib/h-core/auth/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "lib/h-core/auth/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "lib/h-core/auth/**/*.ts",
          "lib/h-core/auth/**/*.html",
          "lib/h-core/auth/package.json"
        ]
      }
    }
  }
}
